<h4 class="page-title">Api Key</h4>
<div class="block-area" id="horizontal">
	<div id="notice"><%= notice %></div>
	<h3 class="block-title">Api Key</h3>
	<form class="form-horizontal" role="form">
		<div class="form-group">
		<label class="col-md-2 control-label">Alias</label>
		<div class="col-md-10">
			<p class="form-control-static"><%= @api_key.alias %></p>
		</div>
	</div>
		<div class="form-group">
		<label class="col-md-2 control-label">Key</label>
		<div class="col-md-10">
			<p class="form-control-static"><%= @api_key.key %></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-md-2 control-label">Status</label>
		<div class="col-md-10">
			<p class="form-control-static"><%= @api_key.status %></p>
		</div>
	</div>
		</form>
	<div class="form-group">
		<div class="col-md-10 col-md-offset-2">
			<%= link_to 'Edit', edit_api_key_path(@api_key) %> |
			<%= link_to 'Back', api_keys_path %>
		</div>
	</div>
</div>

